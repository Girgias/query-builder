{
    "name": "girgias/query-builder",
    "description": "A library to build valid SQL queries",
    "keywords": ["SQL","query builder", "database"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Peter Banyard",
            "email": "george.banyard@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1",
        "girgias/php-enums": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "vimeo/psalm": "^3.0",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "Girgias\\QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "prs-4": {
            "Girgias\\Tests\\": "tests/"
        }
    }
}
